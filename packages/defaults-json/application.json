    {
      "name": "App Download",
      "description": "Downloads files from the application's remote storage to the local system. Supports different storage scopes and encryption options.",
      "color": "#7459DE",
      "version": "24.2.0",
      "icon": "download",
      "id": "Core.Application.Download",
      "inputs": 1,
      "outputs": 1,
      "properties": [
        {
          "schema": {
            "title": "Input",
            "type": "object",
            "properties": {
              "inRemotePath": {
                "type": "object",
                "title": "Remote File Path",
                "description": "The absolute path for the file on the remote storage, it can be under the instance scope or current execution (run) scope.",
                "variableType": "String",
                "messageScope": true,
                "customScope": true,
                "jsScope": true,
                "properties": {
                  "scope": {
                    "type": "string"
                  },
                  "name": {
                    "type": "string"
                  }
                }
              },
              "inLocalPath": {
                "type": "object",
                "title": "Local File Path",
                "description": "The path to save the downloaded file.",
                "variableType": "String",
                "messageScope": true,
                "customScope": true,
                "jsScope": true,
                "properties": {
                  "scope": {
                    "type": "string"
                  },
                  "name": {
                    "type": "string"
                  }
                }
              }
            }
