          "uiSchema": {
            "inRemotePath": {
              "ui:field": "variable"
            },
            "inLocalPath": {
              "ui:field": "variable"
            },
            "ui:order": [
              "inRemotePath",
              "inLocalPath"
            ]
          },
          "formData": {
            "inRemotePath": {
              "scope": "Custom",
              "name": ""
            },
            "inLocalPath": {
              "scope": "Custom",
              "name": ""
            }
          }
        },
        {
          "schema": {
            "title": "Options",
            "type": "object",
            "properties": {
              "optScope": {
                "type": "string",
                "title": "File Scope",
                "description": "Determines whether the file should be accessible within the 'Run Scope' or the 'Instance Scope'. The 'Run Scope' is specific to a single execution of a flow, allowing files to be accessed from the instance's history. The 'Instance Scope' is tied to a particular instance, making files available to all future executions (runs) of that instance.",
                "enum": [
                  "run_scope",
                  "instance_scope"
                ],
                "enumNames": [
                  "Run Scope",
                  "Instance Scope"
                ]
              },
              "optDecryption": {
                "type": "string",
                "title": "Decryption",
                "description": "Select the appropriate decryption method for your file from three available options. If the file is not encrypted, choose 'No Encryption'. For enhanced security, 'Session Encryption' employs a dynamic key generated between the application and the robot, changing with every new flow run and rendering the key inaccessible for later use. Alternatively, a permanent 'Custom AES Key' from your vault can be utilized for decryption, offering a consistent solution across sessions.",
                "enum": [
                  "none",
                  "session_key",
                  "custom_aes_key"
                ],
                "enumNames": [
                  "None",
                  "Session Key",
                  "Custom AES Key"
                ]
              },
              "optAESKey": {
                "type": "object",
                "title": "Custom AES Key",
                "subtitle": "Custom AES Key",
                "categories": [
                  7
                ],
                "messageScope": true,
                "customScope": true,
                "jsScope": true,
                "properties": {
                  "scope": {
                    "type": "string"
                  },
                  "name": {
                    "type": "object",
                    "properties": {
                      "vaultId": {
                        "type": "string"
                      },
                      "itemId": {
                        "type": "string"
                      }
                    }
                  }
                }
              }
            }
          },
          "uiSchema": {
            "optAESKey": {
              "ui:field": "vault"
            },
            "ui:order": [
              "optScope",
              "optDecryption",
              "optAESKey"
            ]
          },
          "formData": {
            "optScope": "run_scope",
            "optDecryption": "none",
            "optAESKey": {
              "scope": "Custom",
              "name": {
                "vaultId": "_",
                "itemId": "_"
              }
            }
          }
        }
      ]
    },
