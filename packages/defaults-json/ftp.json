    {
      "name": "Connect",
      "version": "24.1.5",
      "description": "Establishes a connection to an FTP, SFTP, or FTPS server using the specified credentials and returns a session ID for subsequent operations.",
      "color": "#CDDC39",
      "icon": "network",
      "id": "Core.FTP.Connect",
      "inputs": 1,
      "outputs": 1,
      "properties": [
        {
          "schema": {
            "title": "Input",
            "type": "object",
            "properties": {
              "inHost": {
                "type": "object",
                "title": "Host",
                "description": "The hostname or IP address of the FTP server to connect to. Cannot be empty. Example: 'ftp.example.com' or '192.168.1.100'.",
                "variableType": "String",
                "messageScope": true,
                "customScope": true,
                "jsScope": true,
                "properties": {
                  "scope": {
                    "type": "string"
                  },
                  "name": {
                    "type": "string"
                  }
                }
              },
              "inPort": {
                "type": "object",
                "title": "Port",
                "description": "The port number to connect to on the FTP server. Cannot be zero. Standard ports: FTP (21), SFTP (22), FTPS (990). Example: 21.",
                "variableType": "Integer",
                "messageScope": true,
                "customScope": true,
                "jsScope": true,
                "properties": {
                  "scope": {
                    "type": "string"
                  },
                  "name": {
                    "type": "string"
                  }
                }
              }
            }
          },
          "uiSchema": {
            "inHost": {
              "ui:field": "variable"
            },
            "inPort": {
              "ui:field": "variable"
            }
          },
          "formData": {
            "inHost": {
              "scope": "Custom",
              "name": ""
