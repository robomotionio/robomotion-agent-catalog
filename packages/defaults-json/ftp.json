                "description": "Optional for FTPS, required when connecting with a private key file.",
                "variableType": "String",
                "messageScope": true,
                "customScope": true,
                "jsScope": true,
                "properties": {
                  "scope": {
                    "type": "string"
                  },
                  "name": {
                    "type": "string"
                  }
                }
              }
            }
