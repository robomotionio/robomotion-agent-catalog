                "description": "The protocol type to use for the connection. FTP (unencrypted), SFTP (SSH File Transfer Protocol - secure), or FTPS (FTP over SSL/TLS - secure).",
                "type": "string",
                "enum": [
                  "ftp",
                  "sftp",
                  "ftps"
                ],
                "enumNames": [
                  "FTP",
                  "sFTP",
                  "FTPS"
                ]
              },
              "optSkipTLSVerification": {
                "title": "Skip TLS Verification",
                "description": "When enabled, skips SSL/TLS certificate verification for secure connections (FTPS/SFTP). Use with caution as this reduces security by not verifying server identity.",
                "type": "boolean"
              },
              "optCredentials": {
                "type": "object",
                "title": "Login Credentials",
                "description": "Username and password credentials stored in the vault for authenticating with the FTP server. Both vault and item must be selected.",
                "subtitle": "Login Credentials",
                "category": 1,
                "messageScope": true,
                "customScope": true,
                "properties": {
                  "scope": {
                    "type": "string"
                  },
                  "name": {
                    "type": "object",
                    "properties": {
