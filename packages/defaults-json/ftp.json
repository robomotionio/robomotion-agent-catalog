        {
          "schema": {
            "title": "Output",
            "type": "object",
            "properties": {
              "outSessionId": {
                "type": "object",
                "title": "Session Id",
                "description": "A unique identifier for the established FTP session. Use this session ID in subsequent FTP operations to perform file transfers and directory operations.",
                "variableType": "String",
                "messageScope": true,
                "properties": {
                  "scope": {
                    "type": "string"
                  },
                  "name": {
                    "type": "string"
                  }
